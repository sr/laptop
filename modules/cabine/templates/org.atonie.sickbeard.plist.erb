<?xml version="1.0" encoding="UTF-8"?>
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>org.atonie.sickbeard</string>

    <key>Boxen</key>
    <dict>
      <key>Description</key>
      <string>The automated Usenet download tool.</string>
    </dict>

    <key>ProgramArguments</key>
    <array>
      <!--<string><%= scope.lookupvar "cabine::config::python" %></string>-->
      <string>/usr/bin/python</string>
      <string><%= scope.lookupvar "cabine::sickbeard::executable" %></string>
      <string>--nolaunch</string>
      <string>--config</string>
      <string><%= scope.lookupvar "cabine::sickbeard::configfile" %></string>
    </array>

    <key>RunAtLoad</key>
    <true />

    <key>KeepAlive</key>
    <false />

    <key>WorkingDirectory</key>
    <string><%= scope.lookupvar "cabine::config::sabnzbd_datadir" %></string>

    <key>StandardErrorPath</key>
    <string><%= scope.lookupvar "cabine::config::sickbeard_logdir" %>/console.log</string>

    <key>StandardOutPath</key>
    <string><%= scope.lookupvar "cabine::config::sickbeard_logdir" %>/console.log</string>
</dict>
</plist>
