<?xml version="1.0" encoding="UTF-8"?>
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>org.atonie.sabnzbd</string>

    <key>Boxen</key>
    <dict>
      <key>Description</key>
      <string>The automated Usenet download tool.</string>
    </dict>

    <key>ProgramArguments</key>
    <array>
      <string><%= scope.lookupvar "cabine::config::python" %></string>
      <string><%= scope.lookupvar "sabnzbd::executable" %></string>
      <string>-f</string>
      <string><%= scope.lookupvar "sabnzbd::configfile" %></string>
    </array>

    <key>RunAtLoad</key>
    <true />

    <key>KeepAlive</key>
    <false />

    <key>WorkingDirectory</key>
    <string><%= scope.lookupvar "sabnzbd::datadir" %></string>

    <key>StandardErrorPath</key>
    <string><%= scope.lookupvar "sabnzbd::logdir" %>/console.log</string>

    <key>StandardOutPath</key>
    <string><%= scope.lookupvar "sabnzbd::logdir" %>/console.log</string>
</dict>
</plist>
